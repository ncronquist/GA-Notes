<h1>Create a planet</h1>

<form action="/planets" method="post" class="form-inline">
  <div class="form-group">
    <input type="text" class="form-control" name="planet[name]" placeholder="Planet Name">
    <input type="submit" class="btn btn-primary">
    <%= token_tag form_authenticity_token %>
  </div>
</form>
